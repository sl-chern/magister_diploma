{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start:dev": {
      "cache": false
    },
    "start:debug": {
      "cache": false
    },
    "debug:docker": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "start:docker": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "seed": {
      "cache": false
    }
  }
}
